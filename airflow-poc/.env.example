# AWS Configuration (using local profile)
AWS_PROFILE=qa-in
AWS_DEFAULT_REGION=us-east-1
AWS_S3_BUCKET=your-s3-bucket-name

# TimescaleDB Connection (your existing PostgreSQL)
TIMESCALE_HOST=host.docker.internal
TIMESCALE_PORT=5432
TIMESCALE_DB=your-timescale-database
TIMESCALE_USER=your-timescale-username
TIMESCALE_PASSWORD=your-timescale-password

# Airflow Database Connection (using your existing PostgreSQL)
AIRFLOW_DB_HOST=host.docker.internal
AIRFLOW_DB_PORT=5432
AIRFLOW_DB_NAME=airflow
AIRFLOW_DB_USER=your-postgres-username
AIRFLOW_DB_PASSWORD=your-postgres-password